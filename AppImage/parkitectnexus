
#!/bin/sh

export MONO_PATH=././lib/mono/4.5:././lib/mono/4.0:././lib/mono/3.5:././lib/mono/2.0:././lib/mono/gtk-sharp-2.0:././lib64:././lib
export MONO_CONFIG=././etc/mono/config
export MONO_CFG_DIR=././etc

././bin/mozroots --import --sync
././bin/mono  "././bin/ParkitectNexus.Client.Linux.exe" "$@"



